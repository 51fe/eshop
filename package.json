{
  "name": "next-eshop",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "prettier:format": "prettier --write \"./src/**/*.{ts,tsx}\" --cache",
    "prettier:check": "prettier --check \"./src/**/*.{ts,tsx}\" --cache",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "migrate": "prisma migrate dev --name init",
    "studio": "prisma studio",
    "hook": "stripe listen --forward-to localhost:3000/webhooks/stripe",
    "trigger": "stripe trigger payment_intent.succeeded",
    "email": "cp .env ./node_modules/react-email && email dev --dir src/email --port 3001"
  },
